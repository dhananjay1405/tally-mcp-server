<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyTallyLiveReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVFROMDATE>{fromDate}</SVFROMDATE>
                <SVTODATE>{toDate}</SVTODATE>
                <nunjuck>if targetCompany</nunjuck>
                <SVCURRENTCOMPANY>{{targetCompany | escape}}</SVCURRENTCOMPANY>
                <nunjuck>endif</nunjuck>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyTallyLiveReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>MyPart01</PARTS>
                        <XMLTAG>DATA</XMLTAG>
                    </FORM>
                    <PART NAME="MyPart01">
                        <LINES>MyLine01</LINES>
                        <REPEAT>MyLine01 : MyCollection</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
					<LINE NAME="MyLine01">
						<FIELDS>Fld01,Fld02,Fld03,Fld04,Fld05,Fld06,Fld07,Fld08,Fld09,Fld10</FIELDS>
						<XMLTAG>ROW</XMLTAG>
					</LINE>
					<FIELD NAME="Fld01">
						<SET>$Guid</SET>
						<XMLTAG>F01</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld02">
						<SET>if $$IsEmpty:$Date then $$StrByCharCode:241 else $$PyrlYYYYMMDDFormat:$Date:"-"</SET>
						<XMLTAG>F02</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld03">
						<SET>$VoucherTypeName</SET>
						<XMLTAG>F03</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld04">
						<SET>$VoucherNumber</SET>
						<XMLTAG>F04</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld05">
						<SET>$Reference</SET>
						<XMLTAG>F05</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld06">
						<SET>if $$IsEmpty:$ReferenceDate then "" else $$PyrlYYYYMMDDFormat:$ReferenceDate:"-"</SET>
						<XMLTAG>F06</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld07">
						<SET>$Narration</SET>
						<XMLTAG>F07</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld08">
						<SET>$PartyLedgerName</SET>
						<XMLTAG>F08</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld09">
						<SET>if $$IsInventoryVch:$VoucherTypeName then 1 else 0</SET>
						<XMLTAG>F09</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld10">
						<SET>if $$IsOrderVch:$VoucherTypeName then 1 else 0</SET>
						<XMLTAG>F10</XMLTAG>
					</FIELD>
					<COLLECTION NAME="MyCollection">
						<TYPE>Voucher</TYPE>
						<FETCH>Narration,PartyLedgerName,Reference,ReferenceDate</FETCH>
						<FILTER>Fltr01,Fltr02</FILTER>
					</COLLECTION>
					<SYSTEM TYPE="Formulae" NAME="Fltr01">NOT $IsCancelled</SYSTEM>
					<SYSTEM TYPE="Formulae" NAME="Fltr02">NOT $IsOptional</SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>