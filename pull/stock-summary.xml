<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyTallyLiveReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVFROMDATE>{fromDate}</SVFROMDATE>
                <SVTODATE>{toDate}</SVTODATE>
                <nunjuck>if targetCompany</nunjuck>
                <SVCURRENTCOMPANY>{{targetCompany | escape}}</SVCURRENTCOMPANY>
                <nunjuck>endif</nunjuck>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyTallyLiveReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>MyPart01</PARTS>
                        <XMLTAG>DATA</XMLTAG>
                    </FORM>
                    <PART NAME="MyPart01">
                        <LINES>MyLine01</LINES>
                        <REPEAT>MyLine01 : MyCollection</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLine01">
                        <FIELDS>Fld01,Fld02,Fld03,Fld04,Fld05,Fld06,Fld07,Fld08,Fld09,Fld10</FIELDS>
                        <XMLTAG>ROW</XMLTAG>
                    </LINE>
                    <FIELD NAME="Fld01">
                        <SET>$Name</SET>
                        <XMLTAG>F01</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld02">
                        <SET>if $$IsEqual:$Parent:$$SysName:Primary then "" else $Parent</SET>
                        <XMLTAG>F02</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld03">
                        <SET>if $$IsEmpty:$OpeningBalance then 0 else (if $$IsInwards:$OpeningBalance then $$Number:$$String:$OpeningBalance:"Tail Units" else -$$Number:$$String:$OpeningBalance:"Tail Units")</SET>
                        <XMLTAG>F03</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld04">
                        <SET>if $$IsEmpty:$OpeningValue then 0 else $$Number:$OpeningValue</SET>
                        <XMLTAG>F04</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld05">
                        <SET>if $$IsEmpty:$InwardQuantity then 0 else (if $$IsInwards:$InwardQuantity then $$Number:$$String:$InwardQuantity:"Tail Units" else -$$Number:$$String:$InwardQuantity:"Tail Units")</SET>
                        <XMLTAG>F05</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld06">
                        <SET>if $$IsEmpty:$InwardValue then 0 else $$Number:$InwardValue</SET>
                        <XMLTAG>F06</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld07">
                        <SET>if $$IsEmpty:$OutwardQuantity then 0 else (if $$IsInwards:$OutwardQuantity then $$Number:$$String:$OutwardQuantity:"Tail Units" else -$$Number:$$String:$OutwardQuantity:"Tail Units")</SET>
                        <XMLTAG>F07</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld08">
                        <SET>if $$IsEmpty:$OutwardValue then 0 else $$Number:$OutwardValue</SET>
                        <XMLTAG>F08</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld09">
                        <SET>if $$IsEmpty:$ClosingBalance then 0 else (if $$IsInwards:$ClosingBalance then $$Number:$$String:$ClosingBalance:"Tail Units" else -$$Number:$$String:$ClosingBalance:"Tail Units")</SET>
                        <XMLTAG>F09</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld10">
                        <SET>if $$IsEmpty:$ClosingValue then 0 else $$Number:$ClosingValue</SET>
                        <XMLTAG>F10</XMLTAG>
                    </FIELD>
                    <COLLECTION NAME="MyCollection">
                        <TYPE>Stock Item</TYPE>
                    </COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>