<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyTallyLiveReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVFROMDATE>{fromDate}</SVFROMDATE>
                <SVTODATE>{toDate}</SVTODATE>
                <nunjuck>if targetCompany</nunjuck>
                <SVCURRENTCOMPANY>{{targetCompany | escape}}</SVCURRENTCOMPANY>
                <nunjuck>endif</nunjuck>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyTallyLiveReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
                    <FORM NAME="MyForm">
                        <PARTS>MyPart01</PARTS>
                        <XMLTAG>DATA</XMLTAG>
                    </FORM>
                    <PART NAME="MyPart01">
                        <LINES>MyLine01</LINES>
                        <REPEAT>MyLine01 : MyCollection</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLine01">
                        <FIELDS>Fld01,Fld02,Fld03,Fld04,Fld05,Fld06,Fld07,Fld08,Fld09</FIELDS>
                        <XMLTAG>ROW</XMLTAG>
                    </LINE>
                    <FIELD NAME="Fld01">
                        <SET>$Name</SET>
                        <XMLTAG>F01</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld02">
                        <SET>if $$IsEqual:$Parent:$$SysName:Primary then "" else $Parent</SET>
                        <XMLTAG>F02</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld03">
                        <SET>if $$IsEqual:$_PrimaryGroup:$$SysName:Primary then "$Primary" else $_PrimaryGroup</SET>
                        <XMLTAG>F03</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld04">
                        <SET>if $IsRevenue then "PL" else "BS"</SET>
                        <XMLTAG>F04</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld05">
                        <SET>if $IsDeemedPositive then "Dr" else "Cr"</SET>
                        <XMLTAG>F05</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld06">
                        <SET>if $$IsDebit:$OpeningBalance then -$OpeningBalance else $OpeningBalance</SET>
                        <XMLTAG>F06</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld07">
                        <SET>$CreditTotals</SET>
                        <XMLTAG>F07</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld08">
                        <SET>$DebitTotals</SET>
                        <XMLTAG>F08</XMLTAG>
                    </FIELD>
                    <FIELD NAME="Fld09">
                        <SET>if $$IsDebit:$ClosingBalance then -$ClosingBalance else $ClosingBalance</SET>
                        <XMLTAG>F09</XMLTAG>
                    </FIELD>
                    <COLLECTION NAME="MyCollection">
                        <TYPE>Ledger</TYPE>
                    </COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>