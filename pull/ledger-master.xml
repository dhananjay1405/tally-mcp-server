<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyTallyLiveReport</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <nunjuck>if targetCompany</nunjuck>
                <SVCURRENTCOMPANY>{{targetCompany | escape}}</SVCURRENTCOMPANY>
                <nunjuck>endif</nunjuck>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyTallyLiveReport">
                        <FORMS>MyForm</FORMS>
                    </REPORT>
					<FORM NAME="MyForm">
						<PARTS>MyPart01</PARTS>
						<XMLTAG>DATA</XMLTAG>
					</FORM>
					<PART NAME="MyPart01">
						<LINES>MyLine01</LINES>
						<REPEAT>MyLine01 : MyCollection</REPEAT>
						<SCROLLED>Vertical</SCROLLED>
					</PART>
					<LINE NAME="MyLine01">
						<FIELDS>Fld01,Fld02,Fld03,Fld04,Fld05,Fld06,Fld07,Fld08,Fld09,Fld10,Fld11,Fld12,Fld13,Fld14,Fld15,Fld16,Fld17,Fld18,Fld19,Fld20,Fld21,Fld22,Fld23,Fld24,Fld25,Fld26,Fld27,Fld28,Fld29</FIELDS>
						<XMLTAG>ROW</XMLTAG>
					</LINE>
					<FIELD NAME="Fld01">
						<SET>$Guid</SET>
						<XMLTAG>F01</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld02">
						<SET>$Name</SET>
						<XMLTAG>F02</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld03">
						<SET>if $$IsEqual:$Parent:$$SysName:Primary then "" else $Parent</SET>
						<XMLTAG>F03</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld04">
						<SET>$OnlyAlias</SET>
						<XMLTAG>F04</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld05">
						<SET>$Description</SET>
						<XMLTAG>F05</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld06">
						<SET>$Narration</SET>
						<XMLTAG>F06</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld07">
						<SET>if $IsRevenue then 1 else 0</SET>
						<XMLTAG>F07</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld08">
						<SET>if $IsDeemedPositive then 1 else 0</SET>
						<XMLTAG>F08</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld09">
						<SET>$$StringFindAndReplace:(if $$IsDebit:$OpeningBalance then -$$NumValue:$OpeningBalance else $$NumValue:$OpeningBalance):"(-)":"-"</SET>
						<XMLTAG>F09</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld10">
						<SET>$$StringFindAndReplace:(if $$IsDebit:$ClosingBalance then -$$NumValue:$ClosingBalance else $$NumValue:$ClosingBalance):"(-)":"-"</SET>
						<XMLTAG>F10</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld11">
						<SET>$MailingName</SET>
						<XMLTAG>F11</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld12">
						<SET>if $$IsEmpty:$Address then "" else $$FullList:Address:$Address</SET>
						<XMLTAG>F12</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld13">
						<SET>$LedStateName</SET>
						<XMLTAG>F13</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld14">
						<SET>$CountryName</SET>
						<XMLTAG>F14</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld15">
						<SET>$PinCode</SET>
						<XMLTAG>F15</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld16">
						<SET>$Email</SET>
						<XMLTAG>F16</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld17">
						<SET>$IncomeTaxNumber</SET>
						<XMLTAG>F17</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld18">
						<SET>if $$IsEmpty:$PartyGSTIN then $LedGSTRegDetails[Last].GSTIN else $PartyGSTIN</SET>
						<XMLTAG>F18</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld19">
						<SET>if $$IsEmpty:$Gstregistrationtype then $LedGSTRegDetails[Last].Gstregistrationtype else $Gstregistrationtype</SET>
						<XMLTAG>F19</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld20">
						<SET>$Gsttypeofsupply</SET>
						<XMLTAG>F20</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld21">
						<SET>$Gstdutyhead</SET>
						<XMLTAG>F21</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld22">
						<SET>if $$IsEmpty:$RateOfTaxCalculation then "0" else $$String:$RateOfTaxCalculation</SET>
						<XMLTAG>F22</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld23">
						<SET>$Bankaccholdername</SET>
						<XMLTAG>F23</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld24">
						<SET>$BankDetails</SET>
						<XMLTAG>F24</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld25">
						<SET>$Ifscode</SET>
						<XMLTAG>F25</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld26">
						<SET>$Swiftcode</SET>
						<XMLTAG>F26</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld27">
						<SET>$Bankingconfigbank</SET>
						<XMLTAG>F27</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld28">
						<SET>$BankBranchname</SET>
						<XMLTAG>F28</XMLTAG>
					</FIELD>
					<FIELD NAME="Fld29">
						<SET>$$Number:$BillCreditPeriod</SET>
						<XMLTAG>F29</XMLTAG>
					</FIELD>
					<COLLECTION NAME="MyCollection">
						<TYPE>Ledger</TYPE>
					</COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>